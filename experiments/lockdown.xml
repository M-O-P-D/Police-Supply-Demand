<experiments>
  <experiment name="lockdown" repetitions="1" runMetricsEveryStep="true">
    <setup>
      setup
      py:run "set_loading(1.35, 'drugs')"
      py:run "set_loading(0.65, 'burglary')"
    </setup>
    <go>go-step</go>
    <final>close-files</final>
    <timeLimit steps="9504"/>
    <steppedValueSet variable="replication" first="1" step="1" last="10"/>
    <enumeratedValueSet variable="non-crime-%-RESPONSE">
      <value value="0"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="shift-1-response">
      <value value="50"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="shift-3-CID">
      <value value="10"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="shift-2-CID">
      <value value="30"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="shift-1-CID">
      <value value="30"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="non-crime-%-CID">
      <value value="0.2"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="shift-2-response">
      <value value="50"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="Force">
      <value value="&quot;Durham&quot;"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="event-file-out">
      <value value="false"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="show-workload">
      <value value="false"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="replication">
